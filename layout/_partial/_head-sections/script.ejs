<!--script-->
<%
var _highlightScript = theme.highlightScript||'/js/thirdParty/highlight.pack.js';
var _leancloud = theme.leancloud_src||'/js/thirdParty/leancloud.min.js';
var _busuanzi = theme.busuanzi_src||'js/thirdParty/busuanzi.min.js';
%>
<%- js(_highlightScript)%>
<script>
    hljs.initHighlightingOnLoad();
</script>
<% if(theme.leancloud&&theme.leancloud.enable && theme.leancloud.app_id && theme.leancloud.app_key){%>
<%- js(_leancloud)%>
<script>
    var appId = "<%- theme.leancloud.app_id%>";
    var appKey = "<%- theme.leancloud.app_key%>";
    AV.init({
        appId: appId,
        appKey: appKey
    });
</script>
<%}%>
<%if(theme.visits_count.enable){%>
<script async src="<%- url_for(_busuanzi)%>"></script>
<%}%>